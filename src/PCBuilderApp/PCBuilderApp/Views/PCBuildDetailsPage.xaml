<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PCBuilderApp.Views.PCBuildDetailsPage"
             xmlns:vm="clr-namespace:PCBuilderApp.ViewModels"
             Title="{Binding PcBuild.PCBuildName}"
             x:DataType="vm:PCBuildDetailsViewModel">

    <StackLayout>
        <Label Text="{Binding PcBuild.PCCpu}" VerticalOptions="Center" HorizontalOptions="Center"></Label>
        <Label Text="{Binding PcBuild.PCCpuCooler}" VerticalOptions="Center" HorizontalOptions="Center"></Label>
        <Label Text="{Binding PcBuild.PCGpu}" VerticalOptions="Center" HorizontalOptions="Center"></Label>
        <Label Text="{Binding PcBuild.PCMotherboard}" VerticalOptions="Center" HorizontalOptions="Center"></Label>
        <Label Text="{Binding PcBuild.PCRam}" VerticalOptions="Center" HorizontalOptions="Center"></Label>
        <Label Text="{Binding PcBuild.PCStorage}" VerticalOptions="Center" HorizontalOptions="Center"></Label>
        <Label Text="{Binding PcBuild.PCPsu}" VerticalOptions="Center" HorizontalOptions="Center"></Label>
        <Label Text="{Binding PcBuild.PCCase}" VerticalOptions="Center" HorizontalOptions="Center"></Label>
    </StackLayout>
</ContentPage>